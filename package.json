{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node build/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess \"node ./build/server.js\"",
    "build": "npx tsc",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "lint": "eslint . --ext .ts",
    "migrate": "db-migrate --env test up && db-migrate up",
    "migratereset": "db-migrate --env test reset && db-migrate reset",
    "jasmine": "jasmine",
    "test": "set ENV=test && db-migrate reset && db-migrate up && jasmine",
    "test1": "db-migrate reset && db-migrate up && jasmine",
    "test2": "set ENV=test && db-migrate --env test reset && db-migrate --env test up && jasmine && db-migrate --env test reset"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/pg": "^8.6.5",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "postgres": "^3.2.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/jasmine": "^3.10.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "db-migrate-pg": "^1.2.2",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "supertest": "^6.2.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.7.4"
  }
}
